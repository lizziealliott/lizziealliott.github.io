{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
    "text": "Total IMF Credit Outstanding ",
    "subtitle": "Credit Outstanding for all members, SDRs (billions), Source:IMF",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "black"
    },
    "data": {
    "url": "https://raw.githubusercontent.com/lizziealliott/lizziealliott.github.io/main/extcred1.csv"
   },
  "transform": [{"calculate": "datum.Totals/1000000000", "as": "billions"}],
  "height": 300,
  "width": 300,
  "mark": {"type": "bar", "color": "rgb(0,47,167"},
  "encoding": {
    "x": {
      "field": "Year",
      "type": "temporal",
      "axis": {"title": null, "grid": false, "ticks": false, "labelAngle": 45}
    },
    "y": {
      "field": "billions",
      "type": "quantitative",
      "title": "",
      "axis": {"grid": false}
    }
  }
}